% AHP Potassium current
% Parameters
gAHP = [0.054]
EK = [-80]
b = [2.5]
IC=0
IC_noise=0

% Functions
f(cai) = 1.25*10^8*cai.^2
mtau(cai) = 1000./f(cai)+b
minf(cai) = f(cai)./(f(cai)+b)
IAHP(X,m) = gAHP*m.^2.*(X-EK)

% ODEs and ICs 
m' = (minf(@cai)-m)./mtau(@cai)
m(0) = IC+IC_noise.*rand(1,Npop)

% Linkers
@current += -IAHP(X,m)
@iko += IAHP(X,m)

